
  <%= form_with url: user_friendships_path(user), method: :patch do |f| %>
    <%= inivite(@user)%>

    <%= f.submit 'Accept request' %>
  <% end %>
  <%= form_with url: user_friendships_path(user), method: :delete do |f| %>
